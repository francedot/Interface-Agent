{
  "name": "navaiguide-ts",
  "version": "0.0.2",
  "description": "NavAIGuide: Extensible components toolkit for integrating LLM Web Agents and Browser Companions, enhancing web interfaces with intelligent AI capabilities.",
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/francedot/navaiguide"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build-proj": "tsc -p tsconfig.proj.json",
    "test": "jest",
    "copy-publish-config": "cp publish-package.json package.json",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "prepare": "yarn run build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "clean": "rimraf dist"
  },
  "author": "Francesco Bonacci <francesco.bonacci@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@swc/core": "^1.3.107",
    "@swc/jest": "^0.2.34",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "dotenv": "^16.4.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --cache --fix"
    ],
    "*.md": "prettier --write"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "html-entities": "^2.4.0"
  },
  "files": [
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "main": "./lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "langchain",
    "llm",
    "web",
    "AI"
  ]
}
