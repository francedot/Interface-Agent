{
  "name": "@osagent/windows",
  "version": "0.0.5",
  "description": "Windows integration for OSAgent, enabling powerful desktop automation and testing capabilities.",
  "private": false,
  "scripts": {
    "build": "tsc && shx cp ./src/WinAutomation.psm1 ./lib/WinAutomation.psm1",
    "test": "mocha",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "prepare": "yarn run build",
    "start": "node lib/index.js",
    "console": "npx ts-node apps/console.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/francedot/osagent-ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "osagent",
    "win",
    "llm",
    "windows",
    "AI",
    "langchain",
    "gpt",
    "chatgpt"
  ],
  "author": "Francesco Bonacci <francesco.bonacci@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "@osagent/core": "^0.0.5"
  },
  "files": [
    "lib/"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "main": "./lib/index.js",
  "types": "lib/index.d.ts"
}
